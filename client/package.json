{
  "name": "@reactive-analytics/client",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "scripts": {
    "start": "vite",
    "start:dev": "cross-env VITE_ANALYTICS_SERVER_HOST=dev-reactive-analytics.adaptivecluster.com VITE_ANALYTICS_SERVER_HTTPS=true yarn start",
    "start:demo": "cross-env VITE_ANALYTICS_SERVER_HOST=demo-reactive-analytics.adaptivecluster.com VITE_ANALYTICS_SERVER_HTTPS=true yarn start",
    "start:openfin": "concurrently \"yarn start\" \"yarn openfin\"",
    "start:openfin:dev": "concurrently \"yarn start:dev\" \"yarn openfin\"",
    "build": "vite build",
    "build:dev": "VITE_ANALYTICS_SERVER_HOST=dev-reactive-analytics.adaptivecluster.com VITE_ANALYTICS_SERVER_HTTPS=true vite build",
    "build:demo": "VITE_ANALYTICS_SERVER_HOST=demo-reactive-analytics.adaptivecluster.com VITE_ANALYTICS_SERVER_HTTPS=true vite build",
    "preview": "PORT=3005 vite preview",
    "typecheck": "tsc",
    "lint": "eslint .",
    "_format": "prettier README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "npm run _format -- --write",
    "format:check": "npm run _format -- --check",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "verify": "yarn typecheck && yarn lint && yarn format:check && yarn test",
    "openfin": "wait-on -l http://localhost:3005/openfin/app.json && openfin -l -c http://localhost:3005/openfin/app.json",
    "e2e:web": "playwright test --project=chrome"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@finos/fdc3": "^1.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@openfin/core": "31.75.4",
    "downshift": "^4.0.7",
    "graphql": "16.6.0",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "recharts": "2.3.1",
    "ress": "^2.0.4",
    "styled-components": "^6.0.8",
    "subscriptions-transport-ws": "^0.9.16",
    "typeface-merriweather": "^0.0.72",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@playwright/test": "1.39.0",
    "@rollup/plugin-graphql": "^2.0.2",
    "@rollup/plugin-replace": "^5.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^14.14.37",
    "@types/numeral": "0.0.26",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.27",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "0.32.2",
    "@babel/core": "^7.23.0",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.2",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^21.1.1",
    "openfin-cli": "^4.0.0",
    "polished": "^3.2.0",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "vite": "^4.0.0",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-static-copy": "0.15.0",
    "vitest": "0.32.2",
    "wait-on": "^7.0.0"
  },
  "eslintIgnore": [
    "coverage",
    "dist",
    "public"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
